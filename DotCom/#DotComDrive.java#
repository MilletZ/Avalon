import java.util.ArrayList;
public class DotComDrive {
    public static void main(String[] args) {
	DotCom dc = new DotCom();
	ArrayList<String> dcLocation = new ArrayList<String>();
	dcLocation.add("3");
	dcLocation.add("4");
	dcLocation.add("5");
	
	String dcname = "dot.com";
	String userInput = "3";

	dc.setName(dcname);
	dc.setLocationCells(dcLocation);
	String result = dc.checkYourself(userInput);
	if(result.equals("hit"))
	    System.out.println("passed");
	else
	    System.out.println("failed");
    }
}
